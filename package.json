{
  "name": "lodash-doc-src",
  "version": "0.0.1",
  "description": "pub-server source for lodash documentation",
  "main": "pub-config.js",
  "dependencies": {
    "docdown": "^0.3.0",
    "lodash": "^3.10.1",
    "page": "^1.6.4",
    "pub-server": "^1.9.16"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jldec/lodash-doc-src.git"
  },
  "scripts": {
    "develop": "pub",
    "debug": "pub -D",
    "generate": "pub -O -E",
    "static": "pub -S out",
    "push": "git commit -am \"Update\" && git push",
    "publish": "npm run generate && cd lodash-zh && git add . && git commit -m \"publish\" && git push"
  },
  "author": "jurgen leschner",
  "license": "MIT"
}
